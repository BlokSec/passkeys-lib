{
  "name": "@simplewebauthn/browser",
  "version": "2.1.0",
  "description": "SimpleWebAuthn for Browsers",
  "main": "dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js"
  },
  "types": "./dist/types/index.d.ts",
  "author": "Matthew Miller <matthew@millerti.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MasterKale/SimpleWebAuthn.git",
    "directory": "packages/browser"
  },
  "homepage": "https://github.com/MasterKale/SimpleWebAuthn/tree/master/packages/browser#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all -l clean -p build:esm build:bundles build:declarations",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'dist/esm' --source-maps",
    "build:bundles": "cross-env BABEL_ENV=esmBundled rollup -c",
    "build:declarations": "tsc",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "prepublish": "npm run build"
  },
  "keywords": [
    "typescript",
    "webauthn",
    "fido",
    "umd"
  ],
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@simplewebauthn/typescript-types": "file:../typescript-types",
    "ts-loader": "^7.0.5",
    "webpack": "^4.44.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.44.0",
    "rollup-plugin-terser": "^7.0.2",
    "webpack-auto-inject-version": "^1.2.2",
    "webpack-cli": "^3.3.12"
  },
  "gitHead": "33ccf8c6c9add811c87d3089e24156c2342b3498"
}
